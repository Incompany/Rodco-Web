<li class="producto">
	<a class="nombre" href="#chooseCliente/<%= item.Id %>"><%= item.Name %></a>
	<span class="half">c: <%= item.PrecioMinimo__c %></span>
	<span class="half"><%= item.InventarioActual__c %></span>
	<input data-id="<%= item.Id %>" data-resetValue="true" data-type="cantidad" onchange="onItemChange(event)" class="selectionBox" cols="5" size="5" title="#"/>
</li>
